#!/bin/bash

# Create a new branch and send it to /dev/null
# grep the output for a *, which shows the current branch
# Remove the * from the result
branch=`git branch 2>/dev/null|grep -e ^*|tr -d "* "`

# Get a log of all commits that are in $branch but not on edge
commits=`git log edge..$branch`

clip="Branch $branch
@@@
$commits
@@@"

echo "$clip" | xclip
